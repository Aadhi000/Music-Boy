{
  "name": "OP-Music Boy",
  "description": "Open-Source bot to play songs in your Telegram's Group Voice Chat. Powered by OpusTechz.",
  "keywords": ["music", "voicechat", "telegram"],
  "logo": "https://telegra.ph/file/28159f7bf8c768ebc635d.jpg",
  "repository": "https://github.com/Aadhi000/Music-Boy",
  "stack": "container",
  "env": {
    "SESSION_NAME": {
      "description": "Pyrogram string session",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "A bot token from @BotFather",
      "required": true
    },
    "BOT_NAME": {
      "description": "Your MusicPlayer Bot Name.",
      "required": false,
      "value": ""
    },
    "API_ID": {
      "description": "App ID from my.telegram.org/apps",
      "required": true
    },
    "API_HASH": {
      "description": "App hash from my.telegram.org/apps",
      "required": true
    },
    "SUDO_USERS": {
      "description": "List of user IDs counted as admin everywhere (separated by space).",
      "required": true
    },
    "DURATION_LIMIT": {
      "description": "Max audio duration limit for downloads (minutes).",
      "required": true,
      "value": "10"
    },
    "AUTH": {
    "ENABLE": false,
    "PASSWORD": "",
    "USERS": []
    },
    "SPOTDL_DOWNLOADER": true,
    "SPOTIFYDL_DOWNLOADER": false
    }
  }
}
